<Window x:Class="WpfApplication1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="618" Width="819" WindowStartupLocation="CenterScreen">
    <Grid>
        <RichTextBox Height="140" HorizontalAlignment="Left" Margin="12,12,0,0" Name="richTextBox1" VerticalAlignment="Top" Width="273" TextChanged="richTextBox1_TextChanged" />
        <RichTextBox Height="140" HorizontalAlignment="Left" Margin="291,12,0,0" Name="richTextBox2" VerticalAlignment="Top" Width="200" />
        <WebBrowser Height="141" HorizontalAlignment="Left" Margin="12,158,0,0" Name="webBrowser1" VerticalAlignment="Top" Width="273" DataContext="{Binding RelativeSource={RelativeSource PreviousData}}" IsHitTestVisible="True" OpacityMask="#FF751313" />
        <RichTextBox Height="140" HorizontalAlignment="Left" Margin="497,12,0,0" Name="richTextBox3" VerticalAlignment="Top" Width="288" TextChanged="richTextBox3_TextChanged" />
        <TreeView Height="346" HorizontalAlignment="Left" Margin="291,158,0,0" Name="treeView1" VerticalAlignment="Top" Width="494" DataContext="{Binding}" SelectedItemChanged="treeView1_SelectedItemChanged" GotFocus="treeView1_GotFocus" />
    </Grid>
</Window>
